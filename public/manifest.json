{
  "name": "quickToggleTab",
  "description": "快速切换Tab",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_title": "快速切换Tab"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}