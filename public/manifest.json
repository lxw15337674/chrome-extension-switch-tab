{
  "name": "quickToggleTab",
  "description": "快速切换Tab",
  "version": "1.1.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["tabs", "notifications", "storage"],
  "action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_title": "快速切换Tab"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],
  "commands": {
    "switch1": {
      "description": "切换到编号1标签页"
    },
    "switch2": {
      "description": "切换到编号2标签页"
    },
    "switch3": {
      "description": "switch to tab1"
    },
    "switch4": {
      "description": "switch to tab2"
    },
    "switch5": {
      "description": "switch to tab3"
    },
    "switch6": {
      "description": "switch to tab4"
    },
    "switch7": {
      "description": "switch to tab5"
    },
    "switch8": {
      "description": "switch to tab6"
    },
    "switch9": {
      "description": "switch to tab7"
    },
    "set1": {
      "description": "Set the current tab to tab1"
    },
    "set2": {
      "description": "Set the current tab to tab2"
    },
    "set3": {
      "description": "Set the current tab to tab3"
    },
    "set4": {
      "description": "Set the current tab to tab4"
    },
    "set5": {
      "description": "Set the current tab to tab5"
    },
    "set6": {
      "description": "Set the current tab to tab6"
    },
    "set7": {
      "description": "Set the current tab to tab7"
    },
    "set8": {
      "description": "Set the current tab to tab8"
    },
    "set9": {
      "description": "Set the current tab to tab9"
    }
  },
  "background": {
    "service_worker": "js/background.js"
  }
}
