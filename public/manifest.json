{
  "name": "quickToggleTab",
  "description": "快速切换Tab",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_icon": {
      "128": "icon.png"
    },
    "default_title": "快速切换Tab"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "commands": {
    "switch1": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "切换到编号1标签页",
      "global": true
    },
    "switch2": {
      "suggested_key": {
        "default": "Alt+2"
      },
      "description": "切换到编号2标签页",
      "global": true
    },
    "switch3": {
      "suggested_key": {
        "default": "Alt+3"
      },
      "description": "切换到编号3标签页",
      "global": true
    },
    "switch4": {
      "suggested_key": {
        "default": "Alt+4"
      },
      "description": "切换到编号4标签页",
      "global": true
    }
  },
  "background": {
    "service_worker": "js/background.js"
  }
}